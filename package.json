{
  "name": "@taskless/pack",
  "description": "CLI for working with Taskless packs",
  "version": "0.0.12",
  "author": "Taskless",
  "license": "Apache-2.0",
  "repository": "taskless/pack",
  "scripts": {
    "build": "run-s build:vite build:bin",
    "build:bin": "shx chmod +x dist/cli.js",
    "build:vite": "vite build",
    "changeset": "changeset",
    "cli": "tsx src/cli.ts",
    "codegen": "typed-graphql-builder --schema https://api.taskless.io/graphql --output ./src/__generated__/api.ts",
    "codegen:dev": "typed-graphql-builder --schema http://localhost:3000/graphql --output ./src/__generated__/api.ts",
    "husky": "husky",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "prettier": "prettier",
    "syncpack": "syncpack",
    "test": "NODE_OPTIONS='--no-warnings' vitest",
    "tsx": "tsx",
    "xo": "xo"
  },
  "type": "module",
  "main": "dist/cli.cjs",
  "module": "dist/cli.js",
  "exports": {
    ".": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "bin.js",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@taskless/loader": "latest",
    "colorette": "^2.0.20",
    "commander": "^14.0.0",
    "copy-file": "^11.0.0",
    "dotenv": "^16.5.0",
    "got": "^14.4.7",
    "graphql-request": "^7.1.2",
    "graphql-tag": "^2.12.6",
    "jsonpath": "^1.1.1",
    "ky": "1.8.1",
    "mkdirp": "^3.0.1",
    "mktemp": "^1.0.1",
    "msw": "^2.3.5",
    "read-package-up": "^11.0.0",
    "semver-regex": "^4.0.5",
    "tar": "^7.4.3",
    "uint8array-extras": "^1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^20.12.12",
    "execa": "^9.5.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.2.5",
    "shx": "^0.3.4",
    "syncpack": "^12.3.2",
    "tsx": "^4.16.5",
    "type-fest": "^4.23.0",
    "typed-graphql-builder": "^5.0.1",
    "typescript": "^5.4.5",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "xo": "^0.59.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "pack": "bin.js"
  },
  "keywords": []
}

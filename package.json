{
  "name": "@taskless/packcheck",
  "description": "Check your Taskless Packs",
  "version": "0.0.2",
  "author": "Taskless",
  "license": "Apache-2.0",
  "repository": "taskless/loader-js",
  "scripts": {
    "build": "run-s build:vite build:bin",
    "build:bin": "shx chmod +x dist/index.js",
    "build:vite": "vite build",
    "changeset": "changeset",
    "husky": "husky",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "prettier": "prettier",
    "syncpack": "syncpack",
    "test": "NODE_OPTIONS='--no-warnings' vitest",
    "ts2zod": "ts-to-zod",
    "xo": "xo"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "package.json",
    "wasm"
  ],
  "dependencies": {
    "@taskless/loader": "^0.0.25",
    "colorette": "^2.0.20",
    "command-line-args": "^6.0.1",
    "command-line-usage": "^7.0.3",
    "jsonpath": "^1.1.1",
    "msw": "^2.3.5",
    "uint8array-extras": "^1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/command-line-args": "^5.2.3",
    "@types/command-line-usage": "^5.0.4",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^20.12.12",
    "execa": "^9.5.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.2.5",
    "shx": "^0.3.4",
    "syncpack": "^12.3.2",
    "ts-to-zod": "^3.15.0",
    "tsx": "^4.16.5",
    "type-fest": "^4.23.0",
    "typescript": "^5.4.5",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "xo": "^0.59.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "packcheck": "dist/index.js"
  },
  "keywords": []
}
